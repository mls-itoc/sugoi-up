<!doctype html>
<html lang="ja">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Sugoi-UP App</title>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:regular,bold,italic,thin,light,bolditalic,black,medium&amp;lang=en">
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <link rel="stylesheet" href="https://code.getmdl.io/1.1.1/material.grey-orange.min.css">
    <link rel="stylesheet" href="stylesheets/style.css">
  </head>
  <body>
    <div class="su-app mdl-layout mdl-js-layout has-drawer is-upgraded">
      <main class="mdl-layout__content">
        <div class="mdl-grid">

          <div class="mdl-card cook-pic mdl-cell mdl-cell--6-col mdl-cell--4-col-tablet">
            <div class="mdl-card__title mdl-card--expand">

            <div class="mask"></div>

            <div class="drop">
              <div class="cont">
                <i class="material-icons">cloud_upload</i>
                <div class="tit mdl-typography--headline">
                  Drag & Drop
                </div>
                <div class="desc">
                  or
                </div>
                <button class="mdl-button mdl-js-button mdl-button--raised">
                  click here to browse
                </button>
              </div>
              <output id="list"></output>
              <input id="file" name="cook" type="file" />
            </div>

            </div>
            <div class="mdl-card__supporting-text meta mdl-color-text--grey-50">
              <i class="material-icons">panorama</i>
              <div>
                <strong></strong>
              </div>
            </div>
          </div>

          <div class="mdl-card progress mdl-cell mdl-cell--6-col mdl-cell--4-col-tablet">
            <div class="mdl-card__media mdl-color--white mdl-color-text--grey-600">

            <ul class="log-list mdl-list">
            </ul>

            </div>
          </div>

          <div class="mdl-card poem mdl-cell mdl-cell--12-col">
            <div class="quote-container mdl-card__title mdl-color-text--grey-50"></div>
          </div>

        </div>
      </main>
      <div class="mdl-layout__obfuscator"></div>
    </div>
    <script src="https://code.getmdl.io/1.1.1/material.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js"></script>
    <script src="/socket.io/socket.io.js"></script>
    <script src="javascripts/main.js"></script>
  </body>
</html>
